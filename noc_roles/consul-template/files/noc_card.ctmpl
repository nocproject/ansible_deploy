[[inputs.httpjson]]
  name = "noc-card"
  servers = [
  {{ with node -}}
    {{range .Services -}}
        {{ if eq .Service "card" -}}
            "http://{{.Address}}:{{.Port}}/mon/",
        {{- end}}
     {{- end}}
  {{- end}}
  ]
  method = "GET"
  tag_keys = ["service", "instance"]
  tagexclude = ["server"]
