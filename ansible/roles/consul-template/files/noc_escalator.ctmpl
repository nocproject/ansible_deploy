[[inputs.httpjson]]
  name = "noc-escalator"
  servers = [
  {{ with node -}}
    {{range .Services -}}
        {{ if eq .Service "escalator" -}}
            "http://{{.Address}}:{{.Port}}/mon/",
        {{- end}}
     {{- end}}
  {{- end}}
  ]
  method = "GET"
  tag_keys = ["service", "instance"]
  tagexclude = ["server"]
