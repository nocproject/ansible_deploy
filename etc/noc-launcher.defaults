##------------------------------------------------
## noc-launcher default configuration.
##------------------------------------------------
## WARNING: Do not edit this file directly!!!
## Override settings using etc/noc-launcher.conf
##
[main]
logfile  = /var/log/noc/noc-launcher.log
loglevel = info
logsize  = 0
logfiles = 0
pidfile  = /var/log/noc/noc-launcher.pid

[noc-fcgi]
enabled = true

[noc-sae]
enabled = true

[noc-activator]
enabled = true

[noc-classifier]
enabled = true

[noc-correlator]
enabled = true
