##------------------------------------------------
## noc-stomp default configuration.
##------------------------------------------------
## WARNING: Do not edit this file directly!!!
## Override settings using etc/noc-stomp.conf
##
[main]
logfile  = /var/log/noc/noc-stomp.log
loglevel = info
logsize  = 0
logfiles = 0
syslog_host =
pidfile  = /var/run/noc/noc-stomp.pid
heartbeat= false

[stomp]
listen = 127.0.0.1
port = 19705
